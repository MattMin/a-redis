<idea-plugin>
    <id>com.mzyupc.a-redis</id>
    <name>A-Redis</name>
    <vendor email="mzyupc@163.com" url="http://www.github.com/MattMin">MattMin</vendor>

    <description><![CDATA[
    an idea redis desktop manage plugin..................
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.mzyupc.aredis.factory.ARedisToolWindowFactory" id="A-Redis" secondary="true"
                    icon="AllIcons.Debugger.Db_db_object" anchor="right"/>

        <!-- 注册 -->
        <fileEditorProvider implementation="com.mzyupc.aredis.view.editor.ARedisEditorProvider"/>

        <projectService serviceImplementation="com.mzyupc.aredis.view.editor.ARedisFileSystem"/>


    </extensions>



    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>
